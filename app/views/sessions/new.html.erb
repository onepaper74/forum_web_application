<h1>Log In</h1>

<%= form_with url: sessions_path, local: true do |f| %>
  <div>
    <%= label_tag :email %><br>
    <%= email_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>

  <div>
    <%= submit_tag "Log In" %>
  </div>
<% end %>
